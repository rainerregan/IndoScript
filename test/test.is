// A. Testing Variables
cetak("Testing 1: Test Variables");
atur a = 0;
atur b = 0;

a = 2;
b = 3;

a = a + b; // 5
b = b + 5; // 8

cetak(a);
cetak(b);
// Output: 5 8

// B. Testing If-Else
cetak("Testing 2: Test If-Else");
atur c = 0;
atur d = 0;

c = 2;
d = 3;

jika (c == 2) {
  cetak("c is 2");
} dan jika (d == 3) {
  cetak("d is 3");
} lainnya {
  cetak("c is not 2 and d is not 3");
}
// Output: c is 2 d is 3

// Testing Function
cetak("Testing 3: Test Function");

fungsi abc(a, b) {
  kembalikan a + b;
}

atur e = 0;
atur f = 0;

e = 2;
f = 3;

cetak(abc(e, f));

// Testing Loop
cetak("Testing 4: Test Loop");

atur i = 0;

selama (i < 5) {
  cetak(i);
  i = i + 1;
}

// Testing Boolean
cetak("Testing 5: Test Boolean");

atur g = benar;
atur h = salah;

atur true = g != h;
atur false = g == h;

cetak(true);
cetak(false);

// Testing Bukan
cetak("Testing 6: Test Bukan");

atur j = 5;
atur k = 3;

jika (j dan k) {
  cetak("j is not equal to k");
} lainnya {
  cetak("j is equal to k");
}
// Output: j is not equal to k

// Test Dan & Atau
cetak("Testing 7: Test Dan & Atau");

cetak(benar dan salah);
cetak(salah dan salah);
cetak(benar dan benar);

// Test Binary Operator
cetak("Testing 8: Test Operator");

cetak(2 + 3);
cetak(2 - 3);

cetak(10 > 10);
cetak(10 >= 10);

cetak(10 < 10);
cetak(10 <= 10);

// Test for Loop
cetak("Testing 9: Test For Loop");

untuk (atur l = 0; l < 5; l = l + 1) {
  cetak(l);
}