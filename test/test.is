// A. Testing Variables
tampilkan("Testing 1: Test Variables");
atur a = 0;
atur b = 0;

a = 2;
b = 3;

a = a + b; // 5
b = b + 5; // 8

tampilkan(a);
tampilkan(b);
// Output: 5 8

// B. Testing If-Else
tampilkan("Testing 2: Test If-Else");
atur c = 0;
atur d = 0;

c = 2;
d = 3;

jika (c == 2) {
  tampilkan("c is 2");
} dan jika (d == 3) {
  tampilkan("d is 3");
} lainnya {
  tampilkan("c is not 2 and d is not 3");
}
// Output: c is 2 d is 3

// Testing Function
tampilkan("Testing 3: Test Function");

fungsi abc(a, b) {
  kembalikan a + b;
}

atur e = 0;
atur f = 0;

e = 2;
f = 3;

tampilkan(abc(e, f));

// Testing Loop
tampilkan("Testing 4: Test Loop");

atur i = 0;

selama (i < 5) {
  tampilkan(i);
  i = i + 1;
}